<xsd:schema xmlns:ecl="urn:sios.ree.es:code-lists" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="urn:sios.ree.es:code-lists" elementFormDefault="qualified" attributeFormDefault="unqualified">

	<!--_________________________________________________-->
	<xsd:simpleType name="ListaTipoInterconexion">
		<xsd:annotation>
			<xsd:documentation>
				<Definition>Lista de codigos ISO de 3 caracteres permitidos para indicar un tipo de
					interconexion</Definition>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:NMTOKEN">
			<xsd:enumeration value="PRT">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>PRT Portugal</Title>
							<Definition>Codigo ISO 3 caracteres para PORTUGAL</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="FRA">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>FRA Francia</Title>
							<Definition>Codigo ISO 3 caracteres para Francia</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="MAR">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>MAR Marruecos</Title>
							<Definition>Codigo ISO 3 caracteres para Marruecos</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="AND">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>AND Andorra</Title>
							<Definition>Codigo ISO 3 caracteres para Andorra</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="CB">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>CB Control Block</Title>
							<Definition>Codigo ISO 2 caracteres para Control Block</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<!--_________________________________________________-->
	<xsd:simpleType name="ListaTipoNegocio">
		<xsd:annotation>
			<xsd:documentation>
				<Definition>Lista con los codigos de los tipos de negocio</Definition>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:NMTOKEN">
			<xsd:enumeration value="A01">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>A01 Produccion</Title>
							<Definition>Informacion referente a produccion</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="A02">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>A02 Intercambio interno</Title>
							<Definition>Informacion referente a intercambios internos</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="A03">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>A03 Transacciones individuales</Title>
							<Definition>Informacion referente a intercambios externos detallados
								entre areas con capacidad limitada</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="A04">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>A04 Consumo</Title>
							<Definition>Informacion referente al consumo</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="A05">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>A05 Intercambio total externo</Title>
							<Definition>Informacion referente al total de intercambios
								externos</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="A06">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>A06 Intercambio externo con capacidad ilimitada</Title>
							<Definition>Informacion referente a intercambios externos entre areas
								con capacidad ilimitada</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="A07">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>A07 Produccion / Consumo neto</Title>
							<Definition>Produccion / Consumo neto. Los valores llevan signo: +
								produccion, - consumo</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="A08">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>A08 Intercambio interno neto</Title>
							<Definition>Intercambio interno neto. El signo indica el sentido: +
								desde la UP de salida a la de entrada, - a la inversa</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="A09">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>A09 Productor independiente</Title>
							<Definition>Programa referente a un generador de energia
								independiente</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="A10">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>A10 Regulacion terciaria</Title>
							<Definition>Regulacion terciaria (ofertas, asignaciones,
								requerimientos,…)</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="A11">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>A11 Regulacion primaria</Title>
							<Definition>Regulacion primaria</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="A12">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>A12 Regulacion secundaria</Title>
							<Definition>Regulacion secundaria (ofertas, asignaciones,
								requerimientos,…)</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="A13">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>A13 Perfil de carga</Title>
							<Definition>Perfiles de carga calculados por un agregador de datos de
								medidas</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="A14">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>A14 Datos de energia agregada</Title>
							<Definition>Lecturas de medidas recibidas desde un concentrador de datos
								y agregadas y validadas por un agregador de datos de
								medidas</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="A15">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>A15 Perdidas</Title>
							<Definition>Restitucion de perdidas calculadas en una linea de
								interconexion o area</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="A16">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>A16 Transitos</Title>
							<Definition>Flujos de transito entre areas (CBT)</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="A17">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>A17 Desvio facturable</Title>
							<Definition>Desequilibrio de energia calculado por el operador del
								mercado</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="A18">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>A18 Desvio por restricciones tecnicas</Title>
							<Definition>Desequilibrio entre los programas aceptados por el operador
								del sistema debido a restricciones tecnicas y los programas
								notificados por los sujetos</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="A19">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>A19 Desvio del balance de energia</Title>
							<Definition>Desequilibrio entre el programa de un sujeto establecido por
								el operador del mercado despues de casar las ofertas y el programa
								establecido por restricciones tecnicas</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="A20">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>A20 Volumen de desequilibrio</Title>
							<Definition>Desequilibrio entre las lecturas de medidas actuales y el
								programa del sujeto establecido por el operador del
								sistema</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="A21">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>A21 Compensacion de desvios (UCTE)</Title>
							<Definition>Desvios de regulacion en la interconexion</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="A22">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>A22 Regulacion de frecuencia</Title>
							<Definition>Reservas primaria y secundaria</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="A23">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>A23 Desvios programados</Title>
							<Definition>Mercado de desvios (ofertas, asignaciones, requerimientos,
								...)</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="A24">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>A24 Intercambio total</Title>
							<Definition>Total de intercambios internos y externos</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="A25">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>A25 Informacion de Capacidad General</Title>
							<Definition>Informacion sobre la capacidad total disponible en las
								fronteras de los TSO</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="A26">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>A26 Capacidad transferible disponible</Title>
							<Definition>Capacidad disponible tal y como se define en el documento
								ETSO "Definitions of Transfer Capacities in liberalised Electricity
								Markets"</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="A27">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>A27 Capacidad de la red</Title>
							<Definition>Capacidad de la red tal y como se define en el documento
								ETSO "Definitions of Transfer Capacities in liberalised Electricity
								Markets"</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="A28">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>A28 Intercambio total</Title>
							<Definition>Informacion sobre el total intercambiado entre dos TSOs
								(incluyendo transacciones comerciales y programa de compensacion de
								perdidas).</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="A29">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>A29 Capacidad asignada (AAC)</Title>
							<Definition>Capacidad asignada para nominar</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="A45">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>A45 Reserva de potencia adicional a subir</Title>
							<Definition>Reserva de potencia adicional a subir</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z01">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Z01 Tipo Unidad Programacion: Generacion</Title>
							<Definition>Generacion de Regimen Ordinario (Productores participando
								directamente en el mercado o a traves de un agente vendedor, y
								ventas de energia por comercializadores mediante contratos de
								adquisicion de energia con productores de RO) Apartados 2a, 2b, 2c
								Anexo ii Unidades de Programacion P.O. 3.1</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z02">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Z02 Tipo Unidad Programacion: Autoproductor</Title>
							<Definition>Venta Generacion Regimen Especial (Distribuidores). Apartado
								2f. Anexo II Unidades de Programacion P.O. 3.1</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z03">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Z03 Tipo Unidad Programacion: Consumo</Title>
							<Definition>Consumo de bombeo (Productores). Apartado 1c. Anexo II
								Unidades de Programacion P.O. 3.1</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z04">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Z04 Tipo Unidad Programacion: Contrato Bilateral
								internacional</Title>
							<Definition>Contrato bilateral con entrega fisica (Todos los sujetos del
								mercado de produccion)</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z05">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Z05 Tipo Unidad Programacion: Contrato internacional</Title>
							<Definition>Contrato internacional gestionado por REE</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z06">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Z06 Tipo Unidad Programacion: Distribuidor</Title>
							<Definition>Adquisicion de energia Distribuidor (suministro a tarifa) y
								Venta de Energia Distribuidor (Ajustes) Apartado 1a. y Apartado 2g
								del Anexo II Unidades de Programacion P.O. 3.1</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z07">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Z07 Tipo Unidad Programacion: Cliente Cualificado</Title>
							<Definition>Cliente Cualificado</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z08">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Z08 Tipo Unidad Programacion: Comercializador</Title>
							<Definition>Adquisicion de energia para consumo nacional/Venta de
								energia mediante contratos de adquisicion de energia con productores
								RE (comercializadores) Apartados 1d y 2e. Anexo II Unidades de
								Programacion P.O. 3.1</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z09">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Z09 Tipo Unidad Programacion: Sujeto del mercado externo</Title>
							<Definition>Exportacion / importacion de Energia (Todos los sujetos del
								mercado de produccion autorizados).Apartados 1f y 2g. Anexo II
								Unidades de Programacion P.O. 3.1</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z10">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Z10 Tipo Unidad Programacion: Regimen Especial</Title>
							<Definition>Generacion Regimen Especial (Productores,
								Autoproductores)Apartado 2d. Anexo II Unidades de Programacion P.O.
								3.1</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z11">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Z11 Tipo Unidad Programacion: Consumo de servicios
								auxiliares</Title>
							<Definition>Adquisicion de energia para servicios auxiliares no
								alimentados directamente desde las unidades de produccion
								(Productores pertenecientes al RO) Apartado 1e. Anexo II Unidades de
								Programacion P.O. 3.1</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z12">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Z12 Tipo Unidad Programacion: Unidad generica emisiones
								primarias</Title>
							<Definition>Unidad de Programacion Generica asociada a las substas de
								emisiones primarias y transacciones interancionales Apartado 3a.
								Anexo II Unidades de Programacion P.O. 3.1</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z13">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Z13 Tipo Unidad Programacion: Unidad generica subastas contratos
								bilaterales distribuidoras</Title>
							<Definition>Unidad de Programacion Generica asociada a las substas de
								contratos bilaterales de distribuidoras. Apartado 3b. Anexo II
								Unidades de Programacion P.O. 3.1</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z14">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Z14 Reservado</Title>
							<Definition>Reservado</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z15">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Z15 Reservado</Title>
							<Definition>Reservado</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z16">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Z16 Reservado</Title>
							<Definition>Reservado</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z17">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Z17 Tipo Unidad Programacion: Unidad de almacenamiento</Title>
							<Definition>Unidad de Programacion de almacenamiento</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z21">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Z21 Tipo Transaccion: Transacciones de Mercado</Title>
							<Definition>Tipo Transaccion: Transacciones de Mercado</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z22">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Z22 Tipo Transaccion: Acumulacion de energia de unidades con
								derechos transfronterizos</Title>
							<Definition>Tipo Transaccion: Acumulacion de energia de unidades con
								derechos transfronterizos</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z23">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Z23 Tipo Transaccion: Transacciones Mercado a Plazo</Title>
							<Definition>Tipo Transaccion: Transacciones Mercado a Plazo</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z24">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Z24 Tipo Transaccion: Contrato bilateral con entrega
								fisica</Title>
							<Definition>Tipo Transaccion: Contrato bilateral con entrega
								fisica</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z31">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Z31 Solicitud Desasignación de Banda de Regulación
								Secundaria</Title>
							<Definition>Z31 Solicitud Desasignación de Banda de Regulación
								Secundaria</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z32">
				<xsd:annotation>
